    "# Cell 1: Import\n",
    "import pennylane as qml\n",
    "print(f\"PennyLane {qml.__version__} ready!\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 2: Simple circuit\n",
    "dev = qml.device(\"default.qubit\", wires=1)\n",
    "\n",
    "@qml.qnode(dev)\n",
    "def circuit():\n",
    "    qml.Hadamard(0)\n",
    "    return qml.state()\n",
    "\n",
    "print(\"Quantum state:\", circuit())"
   ]
  }
